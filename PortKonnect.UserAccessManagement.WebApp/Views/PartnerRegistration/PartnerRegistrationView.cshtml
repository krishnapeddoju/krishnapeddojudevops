@model PortKonnect.UserAccessManagement.Domain.DTOS.PrivilegeDTO
@{    
   Layout = "~/Views/Shared/_eGateLayout.cshtml";
}

@section scripts
{
<script src="~/Scripts/Application/UAM/Model/ViewPartnerRegistrationModel.js"></script>
<script src="~/Scripts/Application/UAM/ViewModel/PartnerRegistrationViewViewModel.js"></script>

}
@section ko_apply
{
var viewModel = new eGateRoot.PartnerRegistrationViewViewModel('@ViewBag.RequisitionNo','@ViewBag.FormMode');
ko.applyBindingsWithValidation(viewModel, $("#KYCForm")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }});
}

<style>
    .k-upload-status .k-upload-status-total {
        visibility: hidden;
        display: none;
    }

    .k-icon .k-i-tick {
        visibility: hidden;
        display: none;
    }

    .k-upload-status .k-upload-status-total {
        visibility: hidden;
        display: none;
    }

    .k-upload-button > span {
        padding: 8px !important;
    }

    .k-dropzone strong {
        display: none;
    }

    .k-upload-empty > validationError {
        position: relative !important;
    }

    .k-upload-status {
        display: none;
    }

    .k-widget {
        border-color: #F1F6FA;
    }
    .modal-dialog {width: 500px !important;}
      .clipped {
      overflow: hidden;
      white-space: nowrap;
      text-overflow: ellipsis;
    }
      .gridhead {margin-bottom:0px !important;}
</style>


<div id="KYCForm">
    <div data-bind="visible: viewMode() == 'Form', template: { name: 'Form-template' }"></div>
</div>
<div class="clearfix"></div>

<script type="text/html" id="Form-template">
<div class="row">
    <div class="col-md-12">
        <h3 class="page-title"><span id="spnTitle" data-bind="text: '@ViewBag.Title'"></span></h3>
        <ul class="page-breadcrumb breadcrumb"></ul>
    </div>
</div>
<div class="portlet-body">
<div class="table-toolbar">

<div class="portlet portlet-body form" data-bind="loadingWhen: $root.viewModelHelper.isLoading">
<div class="form-wizard">
<div class="form-body form-horizontal">
<div class="form-group" data-bind="with: ViewPartnerRegistrationModel">
<div class="row">
    <div class="col-md-6"><h4 align="right"> Partner Registration : </h4>
    </div>
    <div class="col-md-6">
        <h4 > &nbsp;<span data-bind="text: RequisitionNo"></span></h4>
    </div>
</div>

<fieldset>
    <legend>Partner Selection</legend>
    <div class="row">
        <div class="">
            <div class="form-group">
                <label class="control-label col-md-2">Partner Selection:</label>
                <div class="col-md-5">
                    <div class="radio-list">
                        <input type="radio" id="partVOA" name="partnerselection" value="VesselOperatingAgent" data-bind="checked: PartnerType, event: { click: $root.GetDocTypes }, enable: false">
                        <label class="radio-inline">VOA</label>&nbsp;&nbsp;&nbsp;&nbsp;
                        <input type="radio" id="partCOA" name="partnerselection" value="ContainerOperatorAgent" data-bind="checked: PartnerType, event: { click: $root.GetDocTypes }, enable: false">
                        <label class="radio-inline">COA</label>&nbsp;&nbsp;&nbsp;&nbsp;
                        <input type="radio" id="partCHA" name="partnerselection" value="CustomHouseAgent" data-bind="checked: PartnerType, event: { click: $root.GetDocTypes }, enable: false">
                        <label class="radio-inline">CHA / FF</label>&nbsp;&nbsp;&nbsp;&nbsp;
                        <input type="radio" id="partCFS" name="partnerselection" value="ContainerFreightStation" data-bind="checked: PartnerType, event: { click: $root.GetDocTypes }, enable: false">
                        <label class="radio-inline">CFS</label>&nbsp;&nbsp;&nbsp;&nbsp;
                        <input type="radio" id="partM&R" name="partnerselection" value="MandR" data-bind="checked: PartnerType, event: { click: $root.GetDocTypes }, enable: false">
                        <label class="radio-inline">M&R</label>
                    </div>
                </div>
            </div>
        </div>
    </div>
</fieldset>


<fieldset>
<legend>Establishment Details</legend>
<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Name:<span class="required">*</span></label>
            <div class="col-md-6">
                <div>
                    <input type="text" id="partnerName" name="" class="form-control clipped" data-bind="value: PartnerName, event: { keypress: $root.validationHelper.ValidateAlphabetsWithSpacesAndSpecialChars_keypressEvent, change: $root.CheckUniquePartnerName }, enable: $root.Isview, attr: { title: PartnerName }" maxlength="50">
                    <span class="validationError" data-bind="validationMessage: PartnerName"></span>
                </div>
            </div>

        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Corporate Identification Number (CIN) (in case of company / LLP):<span class="required">*</span></label>
            <div class="col-md-6">
                <input type="text" id="txtCIN" name="" style="text-transform: uppercase" class="form-control" maxlength="21" data-bind="value: CIN, event: { keypress: $root.validationHelper.ValidateAlphaNumeric, change: $root.CheckUniqueCIN }, enable: $root.Isview">

                <span class="validationError" id="validationCIN"></span>
            </div>
        </div>
    </div>

</div>
<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Year of Incorporation:<span class="required">*</span></label>
            <div class="col-md-6">

                <select id="txtYear" name="" class="form-control" data-bind="options: $root.YearsList, optionsCaption: 'Select...', value: Year,enable: $root.Isview"></select>
                <span class="validationError" data-bind="validationMessage: Year"></span>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Permanent Account Number (PAN):<span class="required">*</span></label>
            <div class="col-md-6">
                <div>
                    <input type="text" style="text-transform: uppercase" id="txtPAN" name="" class="form-control" maxlength="10" data-bind="value: PAN, event: { keypress: $root.validationHelper.ValidateAlphaNumeric, change: $root.CheckUniquePAN }, enable: $root.Isview" onchange=" validatePAN() ">
                    <span class="validationError" data-bind="validationMessage: PAN"></span>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Address Line 1:<span class="required">*</span></label>
            <div class="col-md-6" data-bind="with: PartnerRegistrationAddress">
                <div>
                    <input type="text" id="houseNumber" name="" class="form-control clipped" data-bind="value: HouseNumber, event: { keypress: $root.validationHelper.ValidateAlphaNumericWithSpacesAndSpecialCharsForHouseNo_keypressEvent }, enable: $root.Isview, attr: { title: HouseNumber }" onchange=" validateHouseNumber() " maxlength="100">
                    <span class="validationError" id="validationHouseNumber"></span>
                </div>
            </div>

        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Tax Deduction Account Number (TAN):<span class="required" data-bind="visible: PartnerType() != 'CustomHouseAgent'">*</span></label>
            <div class="col-md-6">
                <div>
                    <input type="text" id="txtTAN" style="text-transform: uppercase" name="" class="form-control" maxlength="10" data-bind="value: TAN, event: { keypress: $root.validationHelper.ValidateAlphaNumeric, change: $root.CheckUniqueTAN }, enable: $root.Isview">
                    <span class="validationError" data-bind="validationMessage: TAN"></span>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">

    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Address Line 2:&nbsp;&nbsp;</label>
            <div class="col-md-6" data-bind="with: PartnerRegistrationAddress">
                <input type="text" id="streetName" name="" class="form-control clipped" onchange=" validateStreetName() " maxlength="100" data-bind="value: StreetName, enable: $root.Isview, attr: { title: StreetName }">
                <span class="validationError" id="validationStreetName"></span>
            </div>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Establishment Regn. No.:<span class="required">*</span></label>
            <div class="col-md-6">
                <div>
                    <input type="text" id="registrationNo" name="" class="form-control" maxlength="10" data-bind="value: RegistrationNo, event: { keypress: $root.validationHelper.ValidateAlphaNumeric, change: $root.CheckUniqueRegistrationNo }, enable: $root.Isview">
                    <span class="validationError" data-bind="validationMessage: RegistrationNo"></span>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Address Line 3:&nbsp;&nbsp;</label>
            <div class="col-md-6" data-bind="with: PartnerRegistrationAddress">
                <div>
                    <input type="text" id="areaName" name="" class="form-control clipped" maxlength="100" data-bind="value: AreaName, enable: $root.Isview, attr: { title: AreaName }" onchange=" validateareaName() ">
                    <span class="validationError" id="validationAreaName"></span>
                </div>
            </div>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Registration Place:<span class="required">*</span></label>
            <div class="col-md-6">
                <div>
                    <input type="text" id="txtPlace" name="" class="form-control clipped" maxlength="50" data-bind="value: Place, event: { keypress: $root.validationHelper.ValidateAlphaNumericWithSpaces_keypressEvent }, enable: $root.Isview, attr: { title: Place }">

                    <span class="validationError" data-bind="validationMessage: Place"></span>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Town / City :<span class="required">*</span></label>
            <div class="col-md-6" data-bind="with: PartnerRegistrationAddress">
                <div>
                    <input type="text" id="townOrCity" name="" class="form-control" maxlength="50" data-bind="value: TownOrCity, enable: $root.Isview" onchange=" validatetownOrCity() ">
                    <span class="validationError" id="validationTownOrCity"></span>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">GST / Service Tax / VAT Regn. No.:<span class="required">*</span></label>
            <div class="col-md-6">
                <div>
                    <input type="text" id="txtVAT" name="" style="text-transform: uppercase" class="form-control" maxlength="15" data-bind="value: VAT, event: { keypress: $root.validationHelper.ValidateAlphaNumeric, change: $root.CheckUniqueVAT }, enable: $root.Isview">
                    <span class="validationError" data-bind="validationMessage: VAT"></span>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Country :<span class="required">*</span></label>
            <div class="col-md-6" data-bind="with: PartnerRegistrationAddress">
                <div>
                    <select id="ddlCountry" name="" class="form-control" data-bind="options: $root.Countries, optionsText: 'CountryName', optionsCaption: 'Select...', optionsValue: 'CountryCode', value: Country, event: { change: $root.SelectStates }, enable: $root.Isview" onchange=" validateCountry() "></select>
                    <span class="validationError" id="validationCountry"></span>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Email Id:<span class="required">*</span></label>
            <div class="col-md-6" data-bind="with: PartnerRegistrationAddress">
                <div>
                    <input type="text" id="PartnerEmailId" name="" class="form-control clipped" data-bind="value: Email, event: { keypress: $root.validationHelper.ValidateAlphanumericEmail }, enable: $root.Isview, attr: { title: Email }" maxlength="200" onchange=" validatePartnerEmail() ">
                    <span class="validationError" data-bind="validationMessage: Email"></span>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">State :<span class="required">*</span></label>
            <div class="col-md-6" data-bind="with: PartnerRegistrationAddress">
                <div>
                    <select id="ddlState" name="" class="form-control" data-bind="options: $root.States, optionsText: 'StateName', optionsCaption: 'Select...', optionsValue: 'StateCode', value: State, enable: $root.Isview" onchange=" validateState() "></select>
                    <span class="validationError" id="validationState"></span>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Mobile Number :<span class="required">*</span></label>
            <div class="col-md-6" data-bind="with: PartnerRegistrationAddress">
                <div>
                    <input type="text" id="txtMobileNo" name="" class="form-control" data-bind="value: MobileNumber, event: { keypress: $root.validationHelper.ValidateNumericOnly },enable: $root.Isview" onchange=" validatePartnerMobile() ">
                    <span class="validationError" id="validationPartnerMobile"></span>
                </div>
            </div>
        </div>
    </div>


</div>

<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Zip Code:<span class="required">*</span></label>
            <div class="col-md-6" data-bind="with: PartnerRegistrationAddress">
                <div>
                    <input type="text" id="zipCode" name="" class="form-control" maxlength="6" data-bind="value: ZipCode, event: { keypress: $root.validationHelper.ValidateNumeric }, enable: $root.Isview" onchange=" validatezipCode() ">
                    <span class="validationError" id="validationZipCode"></span>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <label class="control-label col-md-6">Status :<span class="required">*</span></label>
            <div class="col-md-6">
                <div>
                    <select id="ddlStatus" name="" class="form-control" data-bind="options: $root.StatusList, optionsCaption: 'Select...', value: Status, enable: $root.Isview">
                    </select>
                    <span class="validationError" data-bind="validationMessage: Status"></span>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="row">
    <div class="row">
        <div class="col-md-6">
            <div class="row form-group">
                <label class="control-label col-md-6">Logo :<span class="required">&nbsp;&nbsp;</span></label>
                <div class="col-md-6" style="border: 1px solid #ccc; border-radius: 5px;">
                    <table style="width: 100%; height: 20px;">
                        <tr>
                            <td>
                                <img id="sigImage" src="#" height="60" data-bind="visible: (PartnerRegistrationAddress().LogoPath() != '' && PartnerRegistrationAddress().LogoPath() != null)">
                                <span class="validationError" id="validationLogo"></span>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12">
                    <label class="control-label col-md-6"></label>
                    <div class="col-md-6">
                        <span style="color: red">Preferred Dimensions : 90 X 30</span>
                    </div>
                </div>
            </div>
            <div class="row">
                <label class="control-label col-md-6"></label>
                <div class="col-md-6">
                    <table>
                        <tbody data-bind="with: PartnerRegistrationAddress">
                        <tr>
                            <td data-bind="visible: (LogoPath() != '' && LogoPath() != null)">
                                <a href="" id="fileNameLink" data-bind="attr: { href: application.viewbag.appSettings.uamapiUrl + '/FileUploads/Logo/' + LogoPath() }" target="_blank"><span data-bind="    text: LogoFileName"></span></a>

                            </td>
                        </tr>

                        </tbody>
                    </table>
                </div>
            </div>
            <div class="row">
                <label class="control-label col-md-6"></label>
                <div class="col-md-6">
                    <div class="fileupload-buttonbar">
                        <div class="form-group">
                            <div class="col-md-12" data-bind="visible:  $root.Isview">
                                <span class="btn btn-xs green fileinput-button">
                  <i class="fa fa-plus"></i>
                  <span>Browse
                  </span>
                  <input type="file" class="upload" name="files[]" id="fileToUpload" multiple />
              </span>
                                <button type="button" id="CapturePicture" data-icon="upload" data-role="button" data-bind="click: $root.uploadFile" class="btn btn-xs blue start">
                                    <i class="fa fa-upload"></i>
                                    <span>Upload
                  </span>

                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label class="control-label col-md-6">Nature of Business:<span class="required">*</span></label>
                <div class="col-md-6">
                    <div>
                        <input type="text" id="txtNatureOfBusiness" name="" class="form-control" maxlength="50" data-bind="value: NatureOfBusiness, event: { keypress: $root.validationHelper.ValidateAlphaNumericWithSpaces }, enable: $root.Isview">
                        <span class="validationError" data-bind="validationMessage: NatureOfBusiness"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</fieldset>

<fieldset>
    <legend>Details of Directors/Partners/Promoters</legend>
    <div class="row">
        <div class="table-toolbar">
            <div id="tableheader" data-bind="visible:$root.Isview">
                <a class="button gbtngreen" id="btnAddNew" data-bind="click: $root.AddNewDetails"><i class="fa fa-plus"></i>Add New</a>
            </div>
            <div class="table-scrollable">
                <table cellpadding="0" cellspacing="0" border="0" id="tblmovementsdetails" class="table tinytable">
                    <thead>
                    <tr>
                        <th data-bind="visible: $root.Isview">Action</th>
                        <th>Name</th>
                        <th>Permanent Account Number (PAN)</th>
                        <th>Address</th>
                        <th>Mobile</th>
                        <th>Telephone</th>
                        <th>Email</th>
                    </tr>
                    </thead>
                    <tbody data-bind="foreach: PartnerDirectorDetailss">
                    <!-- ko if: Type()=='Director' -->
                    <tr data-bind="css: { trhighlight: ErrorStatus() }">
                        <td style="text-align: center;" data-bind="visible: $root.Isview">
                            <a href='#' id="btnRemove" data-bind='click: $root.RemoveDetails' title="Remove"><i class="fa fa-ban ban-before"></i></a></td>
                        <td>
                            <input type="text" class="form-control clipped" maxlength="20" data-bind="value: PDirectorName, event: { keypress: $root.validationHelper.ValidateAlphabetsWithSpaces_keypressEvent }, enable: $root.Isview, attr: { title: PDirectorName }"></td>
                        <td style="width: 14%">
                            <input type="text" class="form-control" style="text-transform: uppercase" maxlength="10" data-bind="value: PDirectorPAN, event: { keypress: $root.validationHelper.ValidateAlphaNumeric }, enable: $root.Isview"></td>
                        <td style="width: 25%">
                            <textarea class="form-control clipped" style="resize: none;" rows="1" data-bind="value: PDirectorAddress, event: { keypress: $root.validationHelper.ValidateAlphabetsWithSpaces_keypressEvent }, enable: $root.Isview, attr: { title: PDirectorAddress }"></textarea></td>
                        <td style="width: 14%">
                            <div class="col-md-12">
                                <div class="col-md-1" style="text-align: -webkit-right;"> +</div>
                                <div class="col-md-3">
                                    <input type="text" class="form-control" data-bind="value: PCountryCode, event: { keypress: $root.validationHelper.ValidateNumeric }, attr: { id: 'countryCode' + $index() }, enable: $root.Isview" maxlength="3">
                                </div>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" maxlength="15" data-bind="value: PDirectorMobile, event: { keypress: $root.validationHelper.ValidateNumeric }, attr: { id: 'pDirectorMobile' + $index(), title: PDirectorMobile }, enable: $root.Isview">
                                </div>
                            </div></td>
                        <td style="width: 13%">
                            <input type="text" class="form-control" maxlength="11" data-bind="value: PDirectorTele, event: { keypress: $root.validationHelper.ValidateNumeric }, enable: $root.Isview"></td>
                        <td>
                        <input type="text" id="emailId" name="" class="form-control clipped" data-bind="value: PDirectorEmail, event: { keypress: $root.validationHelper.ValidateAlphanumericEmail }, enable: $root.Isview, attr: { title: PDirectorEmail }" maxlength="254">
                    </tr>
                    <!-- /ko -->
                    </tbody>
                </table>
            </div>
        </div>

    </div>
</fieldset>
<fieldset>
    <legend>Bank Details</legend>
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label class="control-label col-md-6">Bank Name:<span class="required">*</span></label>
                <div class="col-md-6">
                    <input type="text" name="" class="form-control clipped" maxlength="20" data-bind="value: BankName, event: { keypress: $root.validationHelper.ValidateAlphabetsWithSpaces_keypressEvent }, enable: $root.Isview, attr: { title: BankName }">
                    <span class="validationError" data-bind="validationMessage: BankName"></span>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label class="control-label col-md-6">Account No.:<span class="required">*</span></label>
                <div class="col-md-6">
                    <input type="text" name="" class="form-control" maxlength="18" data-bind="value: AccountNo, event: { keypress: $root.validationHelper.ValidateNumeric }, enable: $root.Isview">
                    <span class="validationError" data-bind="validationMessage: AccountNo"></span>

                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label class="control-label col-md-6">Bank Address :<span class="required">*</span></label>
                <div class="col-md-6">
                    <textarea name="" class="form-control clipped" maxlength="40" style="resize: none;" data-bind="value: BankAddress, event: { keypress: $root.validationHelper.ValidateAlphaNumericWithSpaces_keypressEvent }, enable: $root.Isview, attr: { title: BankAddress }"></textarea>
                    <span class="validationError" data-bind="validationMessage: BankAddress"></span>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label class="control-label col-md-6">IFSC Code :<span class="required">*</span></label>
                <div class="col-md-6">
                    <input type="text" name="" class="form-control" maxlength="11" data-bind="value: IFSCCode, event: { keypress: $root.validationHelper.ValidateAlphaNumeric }, enable: $root.Isview">

                    <span class="validationError" data-bind="validationMessage: IFSCCode"></span>
                </div>
            </div>
        </div>
    </div>


</fieldset>


<fieldset>
    <legend>Details of Representatives</legend>
    <div class="row">

        <div class="col-md-6">
            <div style="width: 97%;">
                <div class="table-toolbar">

                    <div id="tableheader" data-bind="visible:$root.Isview">
                        <a class="button gbtngreen" id="btnAddNewOperation" data-bind="click: $root.AddNewOperationsDetails"><i class="fa fa-plus"></i>Add New</a>
                    </div>
                    <div class="gridhead">Operations <span class="required">*</span></div>
                    <div class="table-scrollable" style="min-height: 42px;">
                        <table cellpadding="0" cellspacing="0" border="0" id="tblmovementsdetails" class="table tinytable">
                            <thead>
                            <tr>
