                        <input class="btn btn" data-bind="kendoUpload: {
    async: {
        saveUrl: '/api/MultipleFileUpload/DocType_Copy/KPCT',
        autoUpload: true
    }, localization: {
        select: 'Upload Document', dropFilesHere: ''
    }, enabled: $root.IsbuttonsEnable, multiple: false, select: SelectFile, upload: $root.Upload, success: $root.FileSuccess,
    showFileList: false
}, attr: { id: 'upload' + $index() }"
                               type="file" multiple="multiple" title="Upload Document"/></td>
                    <td><a data-bind="text: FileName, attr: { href: '/FileUploads/' + FileName() }" target="_blank" style="text-decoration: underline;"></a></td>
                    <td>

                        <div data-bind="attr: { id: 'IsvalidTill' + $index() }" style="display: none">
